X# Created by: Euan Thoms <euan@potensol.com>
X# $FreeBSD$
X
XPORTNAME=		sope
XPORTVERSION=		2.3.2
XCATEGORIES=		devel gnustep
XMASTER_SITES=		http://www.sogo.nu/files/downloads/SOGo/Sources/
XDISTNAME=		SOPE-${PORTVERSION}
X
XMAINTAINER=		euan@potensol.com
XCOMMENT=		Extensive set of GNUstep web application frameworks
X
XLICENSE=		LGPL20
X
XLIB_DEPENDS=		libxml2.so:${PORTSDIR}/textproc/libxml2 \
X			libmemcached.so:${PORTSDIR}/databases/libmemcached
X
XWRKSRC=			${WRKDIR}/SOPE
X
XOPTIONS_DEFINE=		LDAP MYSQL PGSQL MEMCACHED
XOPTIONS_DEFAULT=	LDAP PGSQL MEMCACHED
XOPTIONS_SUB=		yes
X
XLDAP_DESC=		Build with LDAP support
XMYSQL_DESC=		Build with MySQL support
XPGSQL_DESC=		Build with PostgreSQL support
XMEMCACHED_DESC=		Install memcached
X
XUSES=			gmake gnustep
XUSE_GNUSTEP=		base build
XUSE_LDCONFIG=		${GNUSTEP_LOCAL_LIBRARIES}
X
XSUB_LIST+=              GNUSTEP_LOCAL_TOOLS=${GNUSTEP_LOCAL_TOOLS} \
X                        GNUSTEP_MAKEFILES=${GNUSTEP_MAKEFILES}
X
XCONFIGURE_ARGS=		--with-gnustep --disable-debug --enable-strip
X
X.include <bsd.port.options.mk>
X
X.if ${PORT_OPTIONS:MLDAP}
XUSE_OPENLDAP=	client
X.endif
X
X.if ${PORT_OPTIONS:MPGSQL}
XUSES+=		pgsql
X.endif
X
X.if ${PORT_OPTIONS:MMYSQL}
XUSE_MYSQL=	client
X.endif
X
X.if ${PORT_OPTIONS:MMEMCACHED}
XRUN_DEPENDS+=	${LOCALBASE}/bin/memcached:${PORTSDIR}/databases/memcached
X.endif
X
Xpost-patch:
X.if ! ${PORT_OPTIONS:MLDAP}
X	@${REINPLACE_CMD} -e '/checkLinking "ldap"/d' ${WRKSRC}/configure
X.endif
X.if ! ${PORT_OPTIONS:MPGSQL}
X	@${REINPLACE_CMD} -e '/checkLinking "pq"/d' ${WRKSRC}/configure
X.endif
X.if ! ${PORT_OPTIONS:MMYSQL}
X	@${REINPLACE_CMD} -e '/checkLinking "mysqlclient"/d' ${WRKSRC}/configure
X.endif
X
Xdo-configure:   
X	cd ${WRKSRC} ; . ${GNUSTEP_MAKEFILES}/GNUstep.sh ; ./configure ${CONFIGURE_ARGS}
X
X.include <bsd.port.mk>
